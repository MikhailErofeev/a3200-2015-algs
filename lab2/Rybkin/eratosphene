import math

def sieve(n):
    if n==1:
        return [True,]
    elif n==2:
        rutern [True, True]
    else:
        a = [True, True]
        for x in range(3, n+1):
            m=int(math.sqrt(x))+1
            key=True
            for z in range(2, m):
                if x%z==0:
                    key=False
                    break
            a.append(key)
        return a
